@model IEnumerable<Attendance.Models.Leave>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
           Leave Type
        </th>
        <th>
            &nbsp;&nbsp;&nbsp;Employee ID
        </th>
        <th>
           &nbsp;From
        </th>
        <th>
           &nbsp;To
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LT)
        </td>
        <td>
           &nbsp;&nbsp;&nbsp; @Html.DisplayFor(modelItem => item.Emp_Id)
        </td>
        <td>
           &nbsp; @Html.DisplayFor(modelItem => item.DFrom)
        </td>
        <td>
           &nbsp; @Html.DisplayFor(modelItem => item.DTo)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.L_ID })
        </td>
    </tr>
}

</table>
